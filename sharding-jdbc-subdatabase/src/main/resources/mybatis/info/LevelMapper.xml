<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.info.mapper.LevelMapper">

    <select id="getLevelById" parameterType="long" resultType="com.test.info.entity.Level">
        select id, code, name from level where id = #{id}
    </select>
</mapper>
